#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'WereSync==0.2.1','console_scripts','weresync'
__requires__ = 'WereSync==0.2.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('WereSync==0.2.1', 'console_scripts', 'weresync')()
    )
